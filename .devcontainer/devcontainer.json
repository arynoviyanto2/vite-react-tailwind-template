{
    "name": "Node-Alpine",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode",
                "msjsdiag.vscode-react-native",
                "pkief.material-icon-theme"
            ]
        }
    },
    "forwardPorts": [3000],
    "postCreateCommand": "scripts/startup.sh"
}
